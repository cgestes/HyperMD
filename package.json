{
  "name": "hypermd",
  "version": "0.3.1",
  "description": "A full-featured Markdown editor, breaks the wall between writing and preview.",
  "main": "lib/hypermd_ai1.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build_js && npm run build_css",
