    "@types/marked": "^0.3.0",
    "@types/requirejs": "^2.1.31",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "mathjax": "^2.7.0",
    "rollup": "^0.58.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-typescript2": "^0.13.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sass": "^1.3.2",
    "turndown": "^4.0.1",
    "turndown-plugin-gfm": "^1.0.1",
    "typescript": "^2.8.3"
